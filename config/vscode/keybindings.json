[
  // 検索
  {
    "key": "[Semicolon] r",
    "command": "workbench.action.terminal.searchWorkspace",
    "when": "vim.mode != 'Insert' && !terminalFocus && !findWidgetVisible && !terminalFocus"
  },
  {
    "key": "shift+cmd+f",
    "command": "-workbench.action.terminal.searchWorkspace"
  },
  // ファイル検索
  {
    "key": "[Semicolon] f",
    "command": "workbench.action.quickOpen",
    "when": "vim.mode != 'Insert' && !findWidgetVisible && !terminalFocus"
  },
  {
    "key": "cmd+p",
    "command": "-workbench.action.quickOpen"
  },
  // ファイル作成
  {
    "key": "cmd+n",
    "command": "explorer.newFile",
    "when": "filesExplorerFocus"
  },
  // フォルダ作成
  {
    "key": "cmd+f",
    "command": "explorer.newFolder",
    "when": "filesExplorerFocus"
  },
  // 保存
  {
    "key": "ctrl+s",
    "command": "workbench.action.files.save",
    "when": "editorFocus"
  },
  {
    "key": "cmd+s",
    "command": "-workbench.action.files.save"
  },
  // 矢印禁止
  {
    "key": "up",
    "command": "-extension.vim_up"
  },
  {
    "key": "up",
    "command": "-cursorUp"
  },
  {
    "key": "down",
    "command": "-extension.vim_down"
  },
  {
    "key": "down",
    "command": "-cursorDown"
  },
  {
    "key": "left",
    "command": "-extension.vim_left"
  },
  {
    "key": "left",
    "command": "-cursorLeft"
  },
  {
    "key": "right",
    "command": "-extension.vim_right"
  },
  {
    "key": "right",
    "command": "-cursorRight"
  },
  {
    "key": "ctrl+p",
    "command": "-cursorUp"
  },
  {
    "key": "ctrl+n",
    "command": "-cursorDown"
  },
  // コマンド
  {
    "key": "ctrl+shift+p",
    "command": "workbench.action.quickOpenLeastRecentlyUsedEditorInGroup",
    "when": "!activeEditorGroupEmpty"
  },
  // 開いているファイルへの移動
  // {
  //   "key": "ctrl+shift+tab",
  //   "command": "workbench.action.quickOpenLeastRecentlyUsedEditorInGroup",
  //   "when": "!activeEditorGroupEmpty"
  // },
  {
    "key": "ctrl+shift+n",
    "command": "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
    "when": "!activeEditorGroupEmpty"
  },
  {
    "key": "ctrl+tab",
    "command": "workbench.action.quickOpenNextRecentlyUsedEditorInGroup",
    "when": "!activeEditorGroupEmpty"
  },
  // ナビゲーションの移動
  {
    "key": "ctrl+shift+n",
    "command": "workbench.action.quickOpenNavigateNextInEditorPicker",
    "when": "inEditorsPicker && inQuickOpen"
  },
  {
    "key": "ctrl+tab",
    "command": "workbench.action.quickOpenNavigateNextInEditorPicker",
    "when": "inEditorsPicker && inQuickOpen"
  },
  {
    "key": "ctrl+shift+p",
    "command": "workbench.action.quickOpenNavigatePreviousInEditorPicker",
    "when": "inEditorsPicker && inQuickOpen"
  },
  {
    "key": "ctrl+shift+tab",
    "command": "-workbench.action.quickOpenNavigatePreviousInEditorPicker",
    "when": "inEditorsPicker && inQuickOpen"
  },
  // タブの移動
  {
    "key": "shift+cmd+[",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
  },
  {
    "key": "shift+cmd+[",
    "command": "workbench.action.previousEditor",
    "when": "!terminalFocus"
  },
  {
    "key": "shift+cmd+]",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
  },
  {
    "key": "shift+cmd+]",
    "command": "workbench.action.nextEditor",
    "when": "!terminalFocus"
  },
  // 階層リンク
  {
    "key": "shift+cmd+.",
    "command": "breadcrumbs.focusAndSelect",
    "when": "breadcrumbsPossible"
  },
  {
    "key": "cmd+f",
    "command": "-workbench.action.terminal.focusFind"
  },
  // {
  //   "key": "cmd+f",
  //   "command": "-actions.find"
  // }
  // suggestionの選択
  {
    "key": "ctrl+enter",
    "command": "-github.copilot.generate",
    "when": "editorTextFocus && github.copilot.activated"
  },
  {
    "key": "ctrl+enter",
    "command": "acceptAlternativeSelectedSuggestion",
    "when": "suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "shift+enter",
    "command": "-acceptAlternativeSelectedSuggestion",
    "when": "suggestWidgetVisible && textInputFocus"
  },
  // rename
  {
    "key": "m r",
    "command": "editor.action.rename",
    "when": "editorTextFocus && !editorReadonly && vim.mode != 'Insert'"
  },
  {
    "key": "f2",
    "command": "-editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  // mdのtodo
  {
    "key": "ctrl+Enter",
    "command": "markdown.todo.toggleDone",
    "when": "editorTextFocus && resourceExtname == .md"
  },
  // hover情報の表示
  {
    "key": "shift+k",
    "command": "editor.action.showHover",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "cmd+k cmd+i",
    "command": "-editor.action.showHover",
    "when": "editorTextFocus"
  },
  // quickFix
  {
    "key": "m a",
    "command": "editor.action.quickFix",
    "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly && vim.mode != 'Insert'"
  },
  // copilotの切り替え
  {
    "key": "[Semicolon] shift+c",
    "command": "github.copilot.toggleCopilot",
    "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly && vim.mode != 'Insert' && !findWidgetVisible && !terminalFocus"
  },
  // 戻る
  {
    "key": "g p",
    "command": "workbench.action.navigateBack",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+-",
    "command": "-workbench.action.navigateBack"
  },
  // すすむ
  {
    "key": "g n",
    "command": "workbench.action.navigateForward",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+shift+-",
    "command": "-workbench.action.navigateForward"
  },
  // quickFix
  {
    "key": "cmd+.",
    "command": "-editor.action.quickFix",
    "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
  },
  // 定義ジャンプ
  {
    "key": "g d",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor && vim.mode == 'Normal'"
  },
  {
    "key": "f12",
    "command": "-editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  // 実装ジャンプ
  {
    "key": "g shift+d",
    "command": "editor.action.goToImplementation",
    "when": "editorHasImplementationProvider && editorTextFocus && !isInEmbeddedEditor && vim.mode == 'Normal'"
  },
  {
    "key": "cmd+f12",
    "command": "-editor.action.goToImplementation",
    "when": "editorHasImplementationProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  // エラー箇所へのジャンプ
  {
    "key": "g b",
    "command": "editor.action.marker.prevInFiles",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "g a",
    "command": "editor.action.marker.nextInFiles",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },

  {
    "key": "cmd+enter",
    "command": "filesExplorer.openFilePreserveFocus",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsFolder && !inputFocus"
  },
  {
    "key": "space",
    "command": "-filesExplorer.openFilePreserveFocus",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsFolder && !inputFocus"
  },
  // explorerからファイルへ移動
  {
    "key": "g e",
    "command": "workbench.action.focusFirstEditorGroup",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsFolder && !inputFocus && vim.mode == 'Normal'"
  },
  // explorerに移動
  {
    "key": "g shift+e",
    "command": "workbench.view.explorer",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  // explorerの開閉
  {
    "key": "cmd+s",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "cmd+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  // Go to References
  {
    "key": "g r",
    "command": "editor.action.goToReferences",
    "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor && vim.mode == 'Normal'"
  },
  {
    "key": "shift+f12",
    "command": "-editor.action.goToReferences",
    "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
  },
  // サイドバーの開閉
  {
    "key": "space a",
    "command": "workbench.action.toggleActivityBarVisibility",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  // {
  //   "key": "s v",
  //   "command": "extension.multiCommand.execute",
  //   "args":
  //     "sequence": [
  //       "workbench.action.editorLayoutTwoColumns",
  //       "workbench.action.focusRightGroup"
  //     ]
  //   },
  //   "when": "editorTextFocus && vim.mode == 'Normal'"
  // },
  // 右のエディタに移動
  {
    "key": "g l",
    "command": "workbench.action.focusRightGroup",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "g h",
    "command": "workbench.action.focusLeftGroup",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  // 新しいターミナル
  {
    "key": "cmd+n",
    "command": "workbench.action.terminal.newInActiveWorkspace",
    "when": "terminalFocus"
  },
  // TODO: 横に新しいターミナル
  // {
  //   "key": "cmd+e",
  //   "command": "workbench.action.terminal.newInActiveWorkspace",
  //   "when": "terminalFocus"
  // },
  // windowを全て閉じる
  {
    "key": "cmd+alt+w",
    "command": "workbench.action.closeAllEditors"
  },
  // 移動
  {
    "when": "editorTextFocus && vim.active && !inDebugRepl &&  vim.mode == 'Normal' && !suggestWidgetMultipleSuggestions && !suggestWidgetVisible",
    "key": "k",
    "command": "cursorUp"
  },
  {
    "key": "j",
    "command": "cursorDown",
    "when": "editorTextFocus && vim.active && !inDebugRepl && vim.mode == 'Normal' && !suggestWidgetMultipleSuggestions && !suggestWidgetVisible"
  }
]
